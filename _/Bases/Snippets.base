filters:
  and:
    - '!file.name.endsWith("Template")'
    - categories.containsAny(link("Snippets"))
views:
  - type: table
    name: Snippets
    order:
      - file.name
      - Type
      - tags
    columnSize:
      file.name: 335
      note.isbn: 166
  - type: table
    name: Prompts
    filters:
      and:
        - Type == ["AI Prompt"]
    order:
      - file.name
      - Type
      - tags
    columnSize:
      file.name: 335
      note.isbn: 166
